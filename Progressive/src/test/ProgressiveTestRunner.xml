<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Progressive" parallel="true">
    <test name="progressivepageobjects.HomePage">
        <parameter name="useCloudEnv" value="false"/>
        <parameter name="cloudEnvName" value="browserstack"/>
        <parameter name="os" value="win"/>
        <parameter name="os_version" value="Sierra"/>
        <parameter name="browser_name" value="chrome"/>
        <parameter name="browser_version" value="53.0"/>
        <parameter name="url" value="https://www.progressive.com"/>
        <classes>
            <class name="testprogressivepageobjects.TestHomePage"/>
        </classes>
    </test>
    <test name="progressivepageobjects.HomeQuoteExplorer">
        <parameter name="useCloudEnv" value="false"/>
        <parameter name="cloudEnvName" value="browserstack"/>
        <parameter name="os" value="win"/>
        <parameter name="os_version" value="Sierra"/>
        <parameter name="browser_name" value="firefox"/>
        <parameter name="browser_version" value="53.0"/>
        <parameter name="url" value="https://www.progressive.com/homeowners/homequote-explorer/"/>
        <classes>
            <class name="testprogressivepageobjects.TestHomeQuoteExplorer"/>
        </classes>
    </test>

</suite>